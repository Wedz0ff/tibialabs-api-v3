generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Creature {
  id    String   @id @default(auto()) @map("_id") @db.ObjectId
  name  String
  date  DateTime
}

model Boss {
  id    String   @id @default(auto()) @map("_id") @db.ObjectId
  name  String
  date  DateTime
}
